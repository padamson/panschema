@prefix : <http://example.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@base <http://example.org/ontology/> .

<http://example.org/ontology/> rdf:type owl:Ontology ;
                                dc:title "Example Ontology" ;
                                dc:description "An example ontology for testing rontodoc." ;
                                owl:versionInfo "1.0.0" .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/ontology/knows
:knows rdf:type owl:ObjectProperty ;
       rdfs:domain :Person ;
       rdfs:range :Person ;
       rdfs:comment "A relationship between two people." ;
       rdfs:label "knows" .


#################################################################
#    Data properties
#################################################################

###  http://example.org/ontology/age
:age rdf:type owl:DatatypeProperty ;
     rdfs:domain :Person ;
     rdfs:range xsd:integer ;
     rdfs:comment "The age of a person." ;
     rdfs:label "age" .


#################################################################
#    Classes
#################################################################

###  http://example.org/ontology/Person
:Person rdf:type owl:Class ;
        rdfs:comment "A human being." ;
        rdfs:label "Person" .

###  http://example.org/ontology/Student
:Student rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         rdfs:label "Student" .

###  http://example.org/ontology/Man
:Man rdf:type owl:Class ;
     rdfs:subClassOf :Person ;
     owl:disjointWith :Woman ;
     rdfs:label "Man" .

###  http://example.org/ontology/Woman
:Woman rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       rdfs:label "Woman" .


#################################################################
#    Annotation Properties
#################################################################

###  http://example.org/ontology/definition
:definition rdf:type owl:AnnotationProperty ;
            rdfs:comment "A definition of a term." ;
            rdfs:label "definition" .

#################################################################
#    Named Individuals
#################################################################

###  http://example.org/ontology/John
:John rdf:type owl:NamedIndividual ;
      rdf:type :Person ;
      rdfs:comment "An instance of Person." ;
      rdfs:label "John" .
