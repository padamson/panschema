@prefix : <http://example.org/rontodoc/reference#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology metadata
<http://example.org/rontodoc/reference> a owl:Ontology ;
    rdfs:label "Rontodoc Reference Ontology" ;
    rdfs:comment "A reference ontology for testing rontodoc documentation generation." ;
    owl:versionInfo "0.1.0" .

# Classes
:Animal a owl:Class ;
    rdfs:label "Animal" ;
    rdfs:comment "A living organism that feeds on organic matter." .

:Mammal a owl:Class ;
    rdfs:subClassOf :Animal ;
    rdfs:label "Mammal" ;
    rdfs:comment "A warm-blooded vertebrate with hair or fur." .

:Dog a owl:Class ;
    rdfs:subClassOf :Mammal ;
    rdfs:label "Dog" ;
    rdfs:comment "A domesticated carnivorous mammal." .

:Cat a owl:Class ;
    rdfs:subClassOf :Mammal ;
    rdfs:label "Cat" ;
    rdfs:comment "A small domesticated carnivorous mammal." .

# Object Properties
:hasOwner a owl:ObjectProperty ;
    rdfs:label "has owner" ;
    rdfs:comment "Relates an animal to its owner." ;
    rdfs:domain :Animal ;
    rdfs:range :Person .

:owns a owl:ObjectProperty ;
    rdfs:label "owns" ;
    rdfs:comment "Relates a person to an animal they own." ;
    owl:inverseOf :hasOwner .

# Data Properties
:hasName a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:comment "The name of an entity." ;
    rdfs:range xsd:string .

:hasAge a owl:DatatypeProperty ;
    rdfs:label "has age" ;
    rdfs:comment "The age in years." ;
    rdfs:domain :Animal ;
    rdfs:range xsd:integer .

# Additional class for property range
:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A human being." .

# Example individual
:fido a :Dog ;
    rdfs:label "Fido" ;
    :hasName "Fido" ;
    :hasAge 5 .
